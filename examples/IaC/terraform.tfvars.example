# IBM Cloud API Key
# Get your API key from: https://cloud.ibm.com/iam/apikeys
ibmcloud_api_key = "your-ibm-cloud-api-key-here"

# Region where resources will be deployed
# Options: eu-de, eu-gb, us-south, us-east, jp-tok, au-syd, etc.
region = "eu-de"

# Resource group name (must exist in your IBM Cloud account)
resource_group = "Default"

# Prefix for all resource names
prefix = "demo"

# SSH Public Key
# Generate with: ssh-keygen -t rsa -b 4096 -C "your_email@example.com"
# Get content with: cat ~/.ssh/id_rsa.pub
ssh_public_key = "ssh-rsa AAAAB3NzaC1yc2EAAAADAQABAAACAQ... your-public-key-here"